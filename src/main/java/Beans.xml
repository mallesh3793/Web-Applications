<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
       
       <bean id="petrolEng" class="org.vtl.beans.PetrolEngine" autowire-candidate="false"></bean>
       <bean id="eng" class="org.vtl.beans.DieselEngine" autowire-candidate="true"></bean>
       <bean id="car" class="org.vtl.beans.Car" autowire="byType"></bean>
</beans>
